<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="question" class="com.spring.test.Question">
	<constructor-arg value="101"></constructor-arg>
	<constructor-arg value="What is Java?"></constructor-arg>
	<constructor-arg>
		<map>
			<entry key="Java is a Programming Language"  value="Ravi Kumar"></entry>
			<entry key="Java is a Platform" value="Rishi"></entry>
			<entry key="Java is an Island" value="Amar"></entry>
		</map>
	</constructor-arg>
</bean>

</beans>